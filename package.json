{
  "name": "JobAdvisor",
  "version": "0.0.0",
  "description": "JobAdvisor things. Express.js and Mongoose",
  "main": "app.js",
  "scripts": {
    "install": "chmod -R u+x ./bin/",
    "start": "DEBUG='jobadvisor-server' nodemon ./bin/www",
    "test": "npm run test-mocha && npm start | node seed.js",
    "test-mocha": "JUNIT_REPORT_PATH=./jenkins-test-results.xml istanbul cover --report cobertura _mocha -- --timeout 3000 -R spec --colors --reporter mocha-jenkins-reporter"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "MongoDB",
    "Mongoose",
    "REST"
  ],
  "author": "Francesco Sani, Thomas Del Prete, Alexander Fischer, Samuele Bischof, Lorenzo Spoleti, Davide Bucher, Camillo Malnati, Emanuele Giuseppe Esposito",
  "license": "MIT",
  "dependencies": {
    "adaro": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.14.1",
    "bower": "^1.8.0",
    "debug": "^2.2.0",
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.2",
    "express": "^4.13.3",
    "method-override": "^2.3.5",
    "mocha-jenkins-reporter": "^0.3.6",
    "mongodb": "^2.2.22",
    "mongoose": "^4.1.11",
    "morgan": "^1.6.1",
    "polymer-cli": "^0.17.0",
    "supertest": "^1.1.0"
  },
  "devDependencies": {
    "should": "^4.0.4"
  }
}
